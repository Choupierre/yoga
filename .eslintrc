{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:vue/vue3-recommended",
        "eslint:recommended",
        "@vue/typescript/recommended",
        "./.eslintrc-auto-import.json"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "plugins": [
        //"prettier"
    ],
    "rules": {
        "vue/max-attributes-per-line": [
            "error",
            {
                "singleline": 1,
                "multiline": 1
            }
        ],
        "vue/multi-word-component-names": 0
    }
}